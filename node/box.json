{
   "alias": "rpnode.phar",
   "algorithm": "openssl",
   "chmod": "0755",
   "directories": ["lib"],
   "main": "bin/rpnode",
   "output": "rpnode-0.1.1.phar",
   "stub": true,
   "key": "private.key",
   "finder": [
      {
         "name": "*.php",
         "exclude": [
            "phpunit",
            "phpunit-test-case",
            "Tests",
            "tests"
         ],
         "in": "vendor"
      }
   ],
   "replacements": {
      "node_version": "0.1.1"
   }
}

